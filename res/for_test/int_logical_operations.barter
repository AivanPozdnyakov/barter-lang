func putb(bool f) -> void {
    if f {
        @putd(1);
        return;
    }
    @putd(0);
    return;
}


func main() -> int {
    putb(1<3);
    putb(3<3);
    putb(4<3);

    putb(1<=3);
    putb(3<=3);
    putb(4<=3);

    putb(1>=3);
    putb(3>=3);
    putb(4>=3);

    putb(1==1);
    putb(1==2);

    putb(1!=1);
    putb(1!=2);
    // 1 0 0  1 1 0  0 1 1  1 0  0 1
    return 0;
}